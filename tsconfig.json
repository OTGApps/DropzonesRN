{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "target": "es2015",
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"]
    },
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
